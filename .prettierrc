
{
  "parser": "typescript",
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "semi": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "useTabs": false,
  "htmlWhitespaceSensitivity": "ignore",
  "insertPragma": false,
  "arrowParens": "avoid",
  "requirePragma": false,
  "jsxSingleQuote": false,
  "proseWrap": "always",
  "quoteProps": "as-needed"
}

